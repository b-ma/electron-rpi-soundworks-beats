{
  "name": "electron-rpi-prototyping-boilerplate",
  "version": "1.0.0",
  "description": "Electron - Raspberry Pi prototyping boilerplate",
  "main": "src/main.js",
  "author": "Benjamin Matuszewski",
  "license": "ISC",
  "scripts": {
    "debug": "electron ./src/main.js",
    "deploy": "./bin/runner --deploy",
    "start": "xvfb-run -a electron ./src/main.js",
    "watch": "./node_modules/nodemon/bin/nodemon.js --ignore node_modules --exec \"xvfb-run -a electron ./src/main.js\"",
    "watch-src": "./bin/runner --watch-src"
  },
  "dependencies": {
    "nodemon": "^1.11.0",
    "soundworks": "github:collective-soundworks/soundworks#v2.0.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "colors": "^1.1.2",
    "electron": "^1.6.6",
    "fs-extra": "^3.0.0",
    "klaw": "^1.3.1",
    "nodemon": "^1.11.0",
    "scp2": "^0.5.0",
    "soundworks": "github:collective-soundworks/soundworks#v2.0.0",
    "to-ssh": "^0.5.1",
    "watch": "^1.0.2"
  }
}
